class node:
  def __init__(self,data=None):
    self.left=None
    self.right=None
    self.data=data
  
def bst(root):
  if root!=None:
    if root.left==None and root.right==None:
      return True
    if root.left.data>root.data or root.right.data<root.data:
      return False
    return bst(root.left) and bst(root.right)

root = node(1)
root.left        = node(2)
root.right       = node(3)
root.left.left  = node(4)
root.left.right = node(5) 
a=bst(root)
print(a)
r1=node(15)
r1.left=node(11)
r1.right=node(25)
r1.right.left=node(18)
r1.right.right=node(30)
a=bst(r1)
print(a)

